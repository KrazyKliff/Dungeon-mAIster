{
  "name": "data-models",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "data-models/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project data-models --web",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/data-models",
        "main": "data-models/src/index.ts",
        "tsConfig": "data-models/tsconfig.lib.json",
        "assets": ["data-models/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/data-models"],
      "inputs": [
        "default",
        "sharedGlobals",
        "{workspaceRoot}/jest.preset.js"
      ],
      "options": {
        "jestConfig": "data-models/jest.config.ts"
      }
    }
  }
}
